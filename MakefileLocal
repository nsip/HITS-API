build:
	cd public/lib; cat start.js jquery.rest.js jquery-ui.js purl.js hits.api.js hits.app.edit.js hits.app.list.js hits.config.js hits.school.app.js hits.tag.todo.js hits.school.list.js hits.vendor.info.js end.js > all.js
	cp apidoc/* /var/www/apidoc/

restart:
	sudo apache2ctl restart; tail -f /var/log/apache2/error.log logs/production.log 

